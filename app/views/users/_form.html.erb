<%= form_with model: user do |form| %>
    <div>
        <%= form.label :role %><br>
        <%= form.text_field :role %>
        <% user.errors.full_messages_for(:role).each do |message| %>
            <div><%= message %></div>
        <% end %>
    </div>

    <div>
        <%= form.submit %>
    </div>
<% end %>
